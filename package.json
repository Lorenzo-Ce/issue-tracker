{
  "name": "issuetracker",
  "version": "1.0.0",
  "description": "An issue tracker app",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "workspaceClientBuild": "cd client && npm run build",
    "workspaceClientStart": "cd client && npm run dev",
    "workspaceServerStart": "cd server && npm run start",
    "build": "npm run workspaceClientBuild",
    "start": "concurrently \"npm run workspaceClientStart\" \"npm run workspaceServerStart\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lorenzo-Ce/issue-tracker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Lorenzo-Ce/issue-tracker/issues"
  },
  "homepage": "https://github.com/Lorenzo-Ce/issue-tracker#readme",
  "dependencies": {
    "concurrently": "^7.6.0"
  }
}
